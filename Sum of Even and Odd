import java.util.Scanner;
public class Main {
	
	public static void main(String[] args) {
		// Write your code here
        Scanner s=new Scanner(System.in);
        int N=s.nextInt();
        int even=0,odd=0;
        while(N>0){
            int r=N%10;
            if(r%2==0){
                even=even+r;
            }else{
                odd=odd+r;
            }
            N=N/10;
        }
        System.out.print(even+"\t"+odd);
        

	}
}
